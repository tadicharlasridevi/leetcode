    def isMonotonic(self, nums: List[int]) -> bool:
        return nums==sorted(nums) or nums==sorted(nums,reverse=True)
        
        
        
        
        
        
        
        Example 1:

Input: nums = [1,2,2,3]
Output: true
Example 2:

Input: nums = [6,5,4,4]
Output: true
Example 3:

Input: nums = [1,3,2]
Output: false
